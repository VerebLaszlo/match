#####----------------------------------------------------------------------#####
chi1=chi1all[0:length];                chi1=chi1*1.0;
#theta1=chi1all[length:2*length];       theta1=theta1/pi;
#varphi1=chi1all[2*length:3*length];    varphi1=varphi1/pi;
kappa1=chi1all[3*length:4*length];     kappa1=kappa1/pi;
psi1=chi1all[4*length:5*length];       psi1=psi1/pi;
if (kappa1 != 0 && abs(kappa1) != 1 && psi1 !=  0 && abs(psi1) != 1)\
	psi1s=sprintf(", {/Symbol z}_1=".prec."{/Symbol p}", psi1);
if (kappa1 != 0 && abs(kappa1) != 1 && psi1 == -1)\
	psi1s=", {/Symbol z}_1=-{/Symbol p}";
if (kappa1 != 0 && abs(kappa1) != 1 && psi1 ==  1)\
	psi1s=", {/Symbol z}_1={/Symbol p}";
if (kappa1 != 0 && abs(kappa1) != 1 && psi1 ==  0) psi1s=", {/Symbol z}_1=0";
if (kappa1 != 0 && abs(kappa1) != 1 && psi1 ==  2) psi1s=", {/Symbol z}_1=0";
if (kappa1 !=  0 && abs(kappa1) != 1 )\
	kappa1s=sprintf("{/Symbol k}_1=".prec."{/Symbol p}", kappa1);
if (kappa1 == -1) kappa1s="{/Symbol k}_1=-{/Symbol p}"; psi1s="";
if (kappa1 ==  1) kappa1s="{/Symbol k}_1={/Symbol p}"; psi1s="";
if (kappa1 ==  0) kappa1s="{/Symbol k}_1=0"; psi1s="";
chi2=chi2all[0:length];                chi2=chi2*1.0;
#theta2=chi2all[length:2*length];       theta2=theta2/pi;
#varphi2=chi2all[2*length:3*length];    varphi2=varphi2/pi;
kappa2=chi2all[3*length:4*length];     kappa2=kappa2/pi;
psi2=chi2all[4*length:5*length];       psi2=psi2/pi;
if (kappa2 != 0 && abs(kappa2) != 1 && psi2 !=  0 && abs(psi2) != 1)\
	psi2s=sprintf(", {/Symbol z}_2=".prec."{/Symbol p}", psi2);
if (kappa2 != 0 && abs(kappa2) != 1 && psi2 == -1)\
	psi2s=", {/Symbol z}_2=-{/Symbol p}";
if (kappa2 != 0 && abs(kappa2) != 1 && psi2 ==  1)\
	psi2s=", {/Symbol z}_2={/Symbol p}";
if (kappa2 != 0 && abs(kappa2) != 1 && psi2 ==  0) psi2s=", {/Symbol z}_2=0";
if (kappa2 != 0 && abs(kappa2) != 1 && psi2 ==  2) psi2s=", {/Symbol z}_2=0";
if (kappa2 !=  0 && abs(kappa2) != 1 )\
	kappa2s=sprintf(", {/Symbol k}_2=".prec."{/Symbol p}", kappa2);
if (kappa2 == -1) kappa2s=", {/Symbol k}_2=-{/Symbol p}"; psi2s="";
if (kappa2 ==  1) kappa2s=", {/Symbol k}_2={/Symbol p}"; psi2s="";
if (kappa2 ==  0) kappa2s=", {/Symbol k}_2=0"; psi2s="";
