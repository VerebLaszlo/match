tC1=sysparam[2*length:3*length];       tC1=tC1*1.0;
tC2=sysparam[3*length:4*length];       tC2=tC2*1.0;
tC=tC1>tC2?tC1:tC2;                    tD=abs(tC1-tC2);
tL=tC-2.0*tD;                          if (tL == tC) tL=tL/10.0;
